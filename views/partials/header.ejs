<header>
  <img class="header-img detail-img" src="<%= tmdb[0].img %>" alt="<%= tmdb[0].name %>" />
  <div class="overlay"></div>
  <div>
    <h2><%= tmdb[0].name %></h2>
    <ul>
      <% tmdb[0].genres.forEach(kdrama => { %>
      <li><%= kdrama.charAt(0).toUpperCase() + kdrama.slice(1)%></li>
      <%}); %>
    </ul>
  </div>
</header>
